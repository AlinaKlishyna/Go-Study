scalar UUID

type User {
    userId: UUID!
    username: String!
    email: String!

    meetups: [Meetup!]
}

type Meetup {
    id: ID!
    location: String!
    note: String!
    user: User!
}

type Query {
    meetups: [Meetup!]!
    users: [User!]!

    user(id: UUID!): User
}

input NewUser {
    username: String!
    email: String!
}

type Mutation {
    addUser(input: NewUser): User!
}